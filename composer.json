    "type": "project",
    "require": {
        "php": ">=7.3",
        "propel/propel": "^2.0@dev",
        "ext-json": "*",
        "ramsey/uuid": "^4.0@dev",
        "ext-exif": "*",
        "ext-gd": "*",
        "ext-curl": "*",
        "monolog/monolog": "^2.0@dev",
        "ext-sqlite3": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0@dev"
    },
    "license": "GPLv3",
    "authors": [
        {
            "name": "Alexis Maiquez",
            "email": "almamu@almamu.com"
